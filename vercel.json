{
    "version": 2,
    "builds": [
        {
            "src": "/src/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "package.json",
                    "package-lock.json"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/index.js"
        }
    ]
}